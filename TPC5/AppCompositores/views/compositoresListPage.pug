extends layout 

block content
    .w3-card-4
        h1 
            span Lista de Compositores 
    
    .w3-container
            table.w3-table-all
                tr
                    th Nome
                    th Bio 
                    th Data de Nascimento
                    th Data de Obito
                    th Periodo Musical
                    th Alteracoes
                each compositor in compositores
                    tr 
                        td= compositor.nome
                        td= compositor.bio 
                        td= compositor.dataNasc 
                        td= compositor.dataObito
                        td= compositor.periodo
    footer.w3-container.w3-blue
        h5 Generated by RPCW2024