extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 Entidade: #{entidade},
        
    .w3-container
            table.w3-table-all
                tr
                    th idcontrato
                    th objectoContrato
                    th dataCelebracaoContrato
                    th precoContratual 
                    th NIPC_entidade_comunicante
                    th entidade_comunicante

                each objeto in listaObjetos
                    tr 
                        td
                            a(href="/"+objeto._id)= objeto._id
                        td= objeto.objectoContrato
                        td= objeto.dataCelebracaoContrato
                        td= objeto.precoContratual 
                        td= objeto.NIPC_entidade_comunicante
                        td= objeto.entidade_comunicante
            
    footer.w3-container.w3-teal
    footer.w3-container.w3-teal 
      h5 Generated by RPCW2023 - #[a(href="/") Return]